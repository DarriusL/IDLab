{
    "seed":2024,
    "gpu_is_available":true,
    "known_p_num":10,
    "known_env_num":2,
    "data":{
        "dataset":"v4",
        "train":{
            "dir":"data/v4/train_env0.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":64,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "valid":{
            "dir":"data/v4/valid_env0.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":24,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "test":{
            "dir":"data/v4/test_env0.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":24,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        }
    },
    "train":{
        "is_DA":false,
        "max_epoch":100,
        "valid_start_epoch":5,
        "valid_step":1,
        "stop_train_step_valid_not_improve":30,
        "valid_metrics":"acc",
        "valid_metrics_less":false,
        "optimizer":{
            "lr":1e-5,
            "weight_decay":1e-4
        }
    },
    "model":{
        "name":"DeepWiID",
        "gru_cfg":{
            "hidden_size":256,
            "num_layers":4, 
            "batch_first":true
        },
        "tail_net":{
            "hid_layers":[
                128,
                32
            ],
            "activation_fn":"tanh",
            "drop_out":0.5,
            "end_with_softmax":true
        }
    }
}