{
    "seed":2024,
    "gpu_is_available":true,
    "known_p_num":8,
    "known_env_num":2,
    "data":{
        "dataset":"v3",
        "valid":{
            "dir":"data/v3/valid.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":false,
                "prefetch_factor":4
            }
        },
        "test":{
            "dir":"data/v3/test.mat",
            "loader":{
                "num_workers":4,
                "pin_memory":true,
                "batch_size":24,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        }
    },
    "train":{
        "is_DA":false,
        "max_epoch":50,
        "update_start_epoch":5,
        "update_step":10,
        "valid_metrics":"loss",
        "valid_metrics_less":true,
        "optimizer":{
            "lr":1e-4,
            "weight_decay":1e-4
        }
    },
    "model":{
        "name":"caution",
        "pretrained_enc_dir":"cache/save/v3/caution_encoder_opt1/model_end.pt",
        "initial_threshold":0.5,
        "num_iterations":50, 
        "num_thresholds":20,
        "threshold_step":0.05
    }
}