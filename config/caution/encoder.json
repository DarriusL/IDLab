{
    "seed":2024,
    "gpu_is_available":true,
    "known_p_num":10,
    "known_env_num":2,
    "data":{
        "dataset":"v3",
        "train":{
            "dir":"data/v3/support.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":1,
                "drop_last":false,
                "shuffle":false,
                "prefetch_factor":4
            }
        },
        "valid":{
            "dir":"data/v3/query.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "test":{
            "dir":"data/v3/test_legal.mat",
            "loader":{
                "num_workers":4,
                "pin_memory":true,
                "batch_size":24,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        }
    },
    "train":{
        "is_DA":false,
        "max_epoch":50,
        "update_start_epoch":5,
        "update_step":10,
        "valid_metrics":"loss",
        "valid_metrics_less":true,
        "optimizer":{
            "lr":1e-4,
            "weight_decay":1e-4
        }
    },
    "model":{
        "name":"caution_encoder",
        "do":256,
        "cnn":{
            "hid_layer":[
                32,
                128,
                128
            ],
            "activation_fn":"leaky_relu"
        },
        "FeatureLayer":{
            "hid_layers":[
                512
            ],
            "activation_fn":"tanh",
            "drop_out":0.2,
            "end_with_softmax":false
        }
    }
}