{
    "seed":2024,
    "gpu_is_available":true,
    "known_p_num":10,
    "known_env_num":2,
    "data":{
        "dataset":"v5",
        "train":{
            "dir":"data/v5/train_env0.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "valid":{
            "dir":"data/v5/valid_env0.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "test":{
            "dir":"data/v5/test_env0_pre.mat",
            "loader":{
                "num_workers":4,
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        }
    },
    "train":{
        "is_DA":false,
        "max_epoch":300,
        "valid_start_epoch":1,
        "valid_step":1,
        "stop_train_step_valid_not_improve":50,
        "valid_metrics":"acc",
        "valid_metrics_less":false,
        "optimizer":{
            "lr":1e-5,
            "weight_decay":1e-4
        }
    },
    "model":{
        "name":"BIRDEncoder",
        "is_norm_first":true,
        "d":64,
        "t":64,
        "do":256,
        "TLinear":{
            "hid_layers":[
                4096,
                1024, 
                256
            ],
            "activation_fn":"mish",
            "drop_out":0.5,
            "end_with_softmax":false
        },
        "csinet":{
            "n_refine":2,
            "hid_layers":[
                8,
                16,
                32,
                32,
                16,
                8
            ],
            "activation_fn":"relu"
        },
        "trans2":{
            "max_len":600,
            "is_norm_first":true,
            "d_fc":2048,
            "n_heads": 4,
            "n_layers": 5
        },
        "tail_net":{
            "hid_layers":[
                1024, 
                512,
                256
            ],
            "activation_fn":"mish",
            "drop_out":0.5,
            "end_with_softmax":false
        },
        "p_classifier":{
            "hid_layers":[
                128, 
                64
            ],
            "activation_fn":"mish",
            "drop_out":0.5,
            "end_with_softmax":true
        },
        "env_classifier":{
            "hid_layers":[
                128, 
                64
            ],
            "activation_fn":"mish",
            "drop_out":0.5,
            "end_with_softmax":true
        },
        "lambda_":-1
    }
}