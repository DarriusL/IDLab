{
    "seed":2024,
    "gpu_is_available":true,
    "known_p_num":10,
    "known_env_num":2,
    "data":{
        "dataset":"v5",
        "train":{
            "dir":"data/v5/train_env0.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":24,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "valid":{
            "dir":"data/v5/valid_env0.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "test":{
            "dir":"data/v5/test_env0.mat",
            "loader":{
                "num_workers":4,
                "pin_memory":true,
                "batch_size":24,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        }
    },
    "train":{
