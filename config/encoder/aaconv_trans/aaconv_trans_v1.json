{
    "seed":2024,
    "gpu_is_available":true,
    "known_p_num":10,
    "known_env_num":2,
    "data":{
        "dataset":"v1",
        "train":{
            "dir":"data/v1/train.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            },
            "augmentation":{
                "gaussian_noise":{
                    "type":"gaussian_noise",
                    "snr_db":30
                },
                "multipath_fading":{
                    "type":"multipath_fading",
                    "delay":[
                        [50],
                        [50, 70],
                        [50, 70, 80]
                    ],
                    "coef":[
                        [0.8],
                        [0.8, 0.6],
                        [0.8, 0.6, 0.5]
                    ]
                },
                "freq_selective_fading":{
                    "type":"freq_selective_fading",
                    "scope":[
                        [0.8, 0.3],
                        [0.7, 0.2],
                        [0.6, 0.1],
                        [0.8, 0.1]
                    ]
                },
                "antenna_sequence":{
                    "type":"antenna_sequence"
                },
                "subcarrier_mask":{
                    "type":"subcarrier_mask",
                    "ratio":0.1
                }
            }
        },
        "valid":{
            "dir":"data/v1/valid.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "test":{
            "dir":"data/v1/test_legal.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        }
    },
    "train":{
        "is_DA":false,
        "max_epoch":300,
        "valid_start_epoch":20,
        "valid_step":5,
        "stop_train_step_valid_not_improve":6,
        "valid_metrics":"acc",
        "valid_metrics_less":false,
        "optimizer":{
            "lr":1e-5,
            "weight_decay":1e-4
        }
    },
    "model":{
        "name":"aaconv_trans",
        "is_norm_first":true,
        "d":32,
        "t":32,
        "do":256,
        "TLinear":{
            "hid_layers":[
                4096,
                1024, 
                256
            ],
            "activation_fn":"mish",
            "drop_out":0.5,
            "end_with_softmax":false
        },
        "trans1":{
            "max_len":200,
            "is_norm_first":true,
            "d_fc":2048,
            "n_heads": 3,
            "n_layers": 3
        },
        "aaconv":{
            "hid_layers":[
                8,
                16,
                32,
                16,
                8
            ],
            "activation_fn":"relu"
        },
        "tail_net":{
            "hid_layers":[
                1024, 
                512,
                256
            ],
            "activation_fn":"mish",
            "drop_out":0.5,
            "end_with_softmax":false
        },
        "p_classifier":{
            "hid_layers":[
                128, 
                64
            ],
            "activation_fn":"mish",
            "drop_out":0.5,
            "end_with_softmax":true
        },
        "env_classifier":{
            "hid_layers":[
                128, 
                64
            ],
            "activation_fn":"mish",
            "drop_out":0.5,
            "end_with_softmax":true
        },
        "lambda_":-1
    }
}