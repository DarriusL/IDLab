{
    "seed":2024,
    "gpu_is_available":true,
    "known_p_num":10,
    "known_env_num":2,
    "data":{
        "dataset":"v1",
        "train":{
            "dir":"data/v1/train.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":64,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "valid":{
            "dir":"data/v1/valid.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":24,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "test":{
            "dir":"data/v1/test.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":24,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        }
    },
    "train":{
        "is_DA":false,
        "max_epoch":100,
        "valid_start_epoch":20,
        "valid_step":5,
        "stop_train_step_valid_not_improve":6,
        "valid_metrics":"acc",
        "valid_metrics_less":false,
        "optimizer":{
            "lr":1e-5,
            "weight_decay":1e-4
        }
