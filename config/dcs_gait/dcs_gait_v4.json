{
    "seed":2024,
    "gpu_is_available":true,
    "known_p_num":10,
    "known_env_num":2,
    "data":{
        "dataset":"v4",
        "train":{
            "dir":"data/v4/train.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":16,
                "drop_last":false,
                "shuffle":false,
                "prefetch_factor":4
            }
        },
        "target":{
            "dir":"data/v4/target.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":16,
                "drop_last":false,
                "shuffle":false,
                "prefetch_factor":4
            }
        },
        "valid":{
            "dir":"data/v4/valid.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "test":{
            "dir":"data/v4/test_legal.mat",
            "loader":{
                "num_workers":4,
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        }
    },
    "train":{
        "is_DA":true,
        "max_epoch":100,
        "valid_start_epoch":1,
        "valid_step":1,
        "stop_train_step_valid_not_improve":30,
        "valid_metrics":"acc",
        "valid_metrics_less":false,
        "optimizer":{
            "lr":1e-6,
            "weight_decay":1e-4
        }
    },
    "model":{
        "name":"DCSGait",
        "pretrained_enc_dir":"model/pretrained/DCSGaitEncoder_opt1/best",
        "alpha":1,
        "num_iterations":10,
        "MHA_cfg":{
            "d_q":64,
            "d_k":64,
            "d_v":64,
            "n_heads":3
        },
        "p_classifier":{
            "hid_layers":[
                64
            ],
            "activation_fn":"tanh",
            "drop_out":0.5,
            "end_with_softmax":true
        }
    }
}