{
    "seed":2024,
    "gpu_is_available":true,
    "known_p_num":10,
    "known_env_num":2,
    "data":{
        "dataset":"v1",
        "train":{
            "dir":"data/v1/train.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":32,
                "drop_last":true,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "valid":{
            "dir":"data/v1/valid.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":32,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "test":{
            "dir":"data/v1/test_legal.mat",
            "loader":{
                "num_workers":4,
                "pin_memory":true,
                "batch_size":32,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        }
    },
    "train":{
        "is_DA":false,
        "max_epoch":300,
        "valid_start_epoch":1,
        "valid_step":1,
        "stop_train_step_valid_not_improve":10,
        "valid_metrics":"acc",
        "valid_metrics_less":false,
        "optimizer":{
            "lr":1e-5,
            "weight_decay":1e-4
        }
    },
    "model":{
        "name":"DCSGaitEncoder",
        "do":256,
        "cnn_cfg":{
            "hid_layers":[
                16,
                32
            ]
        },
        "tail_net_cfg":{
            "hid_layers":[
                1024
            ],
            "activation_fn":"tanh",
            "drop_out":0.5,
            "end_with_softmax":false
        },
        "p_classifier":{
            "hid_layers":[
                64
            ],
            "activation_fn":"tanh",
            "drop_out":0.5,
            "end_with_softmax":true
        }
    }
}