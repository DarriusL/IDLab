{
    "seed":2024,
    "gpu_is_available":true,
    "known_p_num":10,
    "known_env_num":2,
    "data":{
        "dataset":"v4",
        "train":{
            "dir":"data/v2/train.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":4,
                "drop_last":true,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "target":{
            "dir":"data/v2/target.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":4,
                "drop_last":true,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "valid":{
            "dir":"data/v2/valid.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "test":{
            "dir":"data/v2/test_legal.mat",
            "loader":{
                "num_workers":4,
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        }
    },
    "train":{
        "is_DA":true,
        "max_epoch":30,
        "valid_start_epoch":1,
        "valid_step":1,
        "stop_train_step_valid_not_improve":10,
        "valid_metrics":"acc",
        "valid_metrics_less":false,
        "optimizer":{
            "lr":1e-5,
            "weight_decay":1e-4
        }
    },
    "model":{
        "name":"WiAiId",
        "do":256,
        "alpha":0.1,
        "beta":0.5,
        "gamma":0.5,
        "MHA_cfg":{
            "d_q":64,
            "d_k":64,
            "d_v":64,
            "n_heads":3
        },
        "MultiScaleCNN_cfg":{
            "h_layers":[
                84
            ],
            "hid_layers":[
                64,
                32,
                8,
                1
            ]
        },
        "p_classifier":{
            "hid_layers":[
                128,
                32
            ]
        },
        "env_classifier":{
            "hid_layers":[
                32
            ]
        }

    }
}