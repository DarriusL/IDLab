{
    "seed":2024,
    "gpu_is_available":true,
    "known_p_num":8,
    "known_env_num":1,
    "data":{
        "dataset":"v6",
        "train":{
            "dir":"data/v6/train_env0.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "valid":{
            "dir":"data/v6/valid_env0.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":16,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "test":{
            "dir":"data/v6/test_env0.mat",
            "loader":{
                "num_workers":4,
                "pin_memory":true,
                "batch_size":8,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        }
    },
    "train":{
        "is_DA":false,
        "max_epoch":100,
        "valid_start_epoch":5,
        "valid_step":1,
        "stop_train_step_valid_not_improve":30,
        "valid_metrics":"loss",
        "valid_metrics_less":true,
        "optimizer":{
            "lr":1e-5,
            "weight_decay":1e-4
        }
    },
    "model":{
        "name":"CAE",
        "loss":"nmse",
        "encoder_hid_layers":[
            128,
            1024,
            1024
        ],
        "decoder_hid_layers":[
            1024,
            1024,
            128
        ]
    }
}