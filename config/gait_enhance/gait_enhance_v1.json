{
    "seed":2024,
    "gpu_is_available":true,
    "known_p_num":10,
    "known_env_num":2,
    "data":{
        "dataset":"v1",
        "train":{
            "dir":"data/v1/train.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":96,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "valid":{
            "dir":"data/v1/valid.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":24,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        },
        "test":{
            "dir":"data/v1/test_legal.mat",
            "loader":{
                "num_workers":4, 
                "pin_memory":true,
                "batch_size":24,
                "drop_last":false,
                "shuffle":true,
                "prefetch_factor":4
            }
        }
    },
    "train":{
        "is_DA":false,
        "max_epoch":150,
        "valid_start_epoch":20,
        "valid_step":5,
        "stop_train_step_valid_not_improve":6,
        "valid_metrics":"acc",
        "valid_metrics_less":false,
        "optimizer":{
            "lr":1e-5,
            "weight_decay":1e-4
        }
    },
    "model":{
        "name":"gait_enhance",
        "window_size":40,
        "blks_cfg":{
            "hid_layers":[
                32,
                64,
                128
            ]
        }, 
        "dropout_rate":0.2,
        "output_layer":{
            "hid_layers":[
                128, 
                64
            ],
            "activation_fn":"tanh",
            "drop_out":0.5,
            "end_with_softmax":true
        }
    }
}